<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<ThingDef>
		<defName>RE_Mechanoids_FENRISBase</defName>
		<label>fenris spawner</label>
		<description>Creates a fenris mech.</description>
		<thingClass>Building</thingClass>
		<graphicData>
			<texPath>Things/Alpha</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<WorkToMake>550</WorkToMake>
			<MaxHitPoints>150</MaxHitPoints>
			<Flammability>0.1</Flammability>
		</statBases>
		<comps>
			<li Class="TabulaRasa.CompProperties_PawnSpawner">
				<pawnKind>RE_Mechanoids_FENRIS</pawnKind>
				<newborn>true</newborn>
			</li>
		</comps>
		<size>(1,1)</size>
		<category>Building</category>
		<pathCost>200</pathCost>
		<passability>Standable</passability>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<filthLeaving>Filth_RubbleRock</filthLeaving>
		<hasInteractionCell>false</hasInteractionCell>
		<drawerType>MapMeshAndRealTime</drawerType>
		<altitudeLayer>Building</altitudeLayer>
		<repairEffect>Repair</repairEffect>
		<useHitPoints>True</useHitPoints>
		<selectable>true</selectable>
		<surfaceType>Item</surfaceType>
		<tickerType>Normal</tickerType>
	</ThingDef>

