<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef ParentName="BuildingBase">
		<defName>RE_PrefabBarricade</defName>
		<label>prefab barricade</label>
		<description>A series of chest-high walls designed to give cover to defenders of an area. If you walk into a room filled with them, you know a fight is about to happen.</description>
		<thingClass>Building</thingClass>
		<category>Building</category>
		<graphicData>
			<texPath>Things/Building/Linked/PrefabBarricades/PrefabBarricade_Atlas</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<linkType>Basic</linkType>
			<linkFlags>
				<li>Barricades</li>
			</linkFlags>
			<color>(139,122,87)</color>
		</graphicData>
		<researchPrerequisites>
			<li>RE_BasicPrefabrication</li>
		</researchPrerequisites>
		<techLevel>Ultra</techLevel>
		<uiIconPath>Things/Building/Linked/PrefabBarricades/PrefabBarricade_MenuIcon</uiIconPath>
		<uiIconScale>1.0</uiIconScale>
		<altitudeLayer>Building</altitudeLayer>
		<pathCost>42</pathCost>
		<pathCostIgnoreRepeat>true</pathCostIgnoreRepeat>
		<passability>PassThroughOnly</passability>
		<castEdgeShadows>true</castEdgeShadows>
		<fillPercent>0.57</fillPercent>
		<canOverlapZones>false</canOverlapZones>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<Beauty>-3</Beauty>
			<WorkToBuild>120</WorkToBuild>
			<Flammability>1</Flammability>
		</statBases>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
			<isInert>true</isInert>
			<blueprintGraphicData>
				<texPath>Things/Building/Linked/PrefabBarricades/PrefabBarricadeBlueprint_Atlas</texPath>
			</blueprintGraphicData>
		</building>
		<placingDraggableDimensions>1</placingDraggableDimensions>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>2</costStuffCount>
		<costList>
			<RE_PrefabComponent>1</RE_PrefabComponent>
		</costList>
		<filthLeaving>Filth_RubbleBuilding</filthLeaving>
		<rotatable>false</rotatable>
		<selectable>true</selectable>
		<neverMultiSelect>true</neverMultiSelect>
		<useStuffTerrainAffordance>true</useStuffTerrainAffordance>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<designationCategory>RE_ArchitectPrefab</designationCategory>
		<modExtensions>
			<li Class="TabulaRasa.DefModExt_SubCategoryBuilding">
				<subCategory>RE_SubCatSecurity</subCategory>
				<showOnlyInCategory>true</showOnlyInCategory>
			</li>
		</modExtensions>
		<staticSunShadowHeight>0.20</staticSunShadowHeight>
	</ThingDef>

</Defs>